{
  "name": "A jQuery Chrome extension",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Use jQuery to build Chrome extensions",
  "content_scripts": [
    {
      "matches": ["https://twitter.com/"],
      "js": ["jquery.js", "content.js"]
    }
  ],
   "permissions": [
    "tabs"
  ],

  "background": "background.html",
  "options_page": "options.html"
}